# GitHub Actions Environment Variables
# Copy this file to .env.github-actions for local testing of CI/CD workflows

# Required for React build
REACT_APP_API_BASE_URL=http://localhost:8080
REACT_APP_AUTH0_DOMAIN=your-auth0-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=your_auth0_audience
BASE_FILE_URL_FOR_IMG=https://your-file-server.com
REACT_APP_NO_IMAGE_AVAILABLE_LOGO=https://your-default-image.com/no-image.png

# Backend variables
PORT=3001
NODE_ENV=development

# Docker build variables
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1
