# Production Environment Variables for Frontend React App
# API Configuration
REACT_APP_API_BASE_URL=https://api.densndent.com
REACT_APP_API_URL=https://api.densndent.com
REACT_APP_API_TIMEOUT=30000

# Auth0 Configuration (Production)
REACT_APP_AUTH0_DOMAIN=prod-your-domain.us.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_production_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=https://api.densndent.com

# NetSuite Configuration (Production)
BASE_FILE_URL_FOR_IMG=https://your-prod-account.app.netsuite.com
REACT_APP_NO_IMAGE_AVAILABLE_LOGO=https://densndent.com/images/no-image-available.png

# VersaPay Configuration (Production)
REACT_APP_VERSAPAY_PUBLISHABLE_KEY=pk_live_your_production_key_here
REACT_APP_VERSAPAY_API_URL=https://api.versapay.com

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_CONSOLE_LOGS=false
REACT_APP_ENABLE_ANALYTICS=true

# Analytics & Monitoring
REACT_APP_GOOGLE_ANALYTICS_ID=GA-PROD-123456
REACT_APP_SENTRY_DSN=your_production_sentry_dsn

# App Configuration
REACT_APP_APP_NAME=DensNDent
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Payment Configuration
REACT_APP_CURRENCY=CAD
REACT_APP_PAYMENT_TIMEOUT=300000
