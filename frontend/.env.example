# Frontend Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001

# Auth0 Configuration (SENSITIVE - Use your actual values)
REACT_APP_AUTH0_DOMAIN=your-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=https://your-domain.auth0.com/api/v2/

# NetSuite File URLs
BASE_FILE_URL_FOR_IMG=https://your-account.app.netsuite.com
REACT_APP_NO_IMAGE_AVAILABLE_LOGO=https://your-domain.com/path/to/no-image-available.png
