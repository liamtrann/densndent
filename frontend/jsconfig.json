{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "components/*": ["components/*"],
      "common/*": ["common/*"],
      "store/*": ["redux/*"],
      "api/*": ["api/*"],
      "constants/*": ["constants/*"],
      "hooks/*": ["hooks/*"],
      "pages/*": ["pages/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
