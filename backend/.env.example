# Backend Environment Variables Example
# Copy this file to .env and fill in your actual values

# Node.js Configuration
NODE_ENV=development
PORT=3001

# NetSuite Configuration (SENSITIVE - Use your actual values)
NETSUITE_BASE_URL=https://your-account.suitetalk.api.netsuite.com
SUITEQL_CONSUMER_KEY=your_consumer_key_here
SUITEQL_CONSUMER_SECRET=your_consumer_secret_here
SUITEQL_ACCESS_TOKEN=your_access_token_here
SUITEQL_TOKEN_SECRET=your_token_secret_here
SUITEQL_REALM=your_realm_here

# Auth0 Configuration (SENSITIVE - Use your actual values)
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret
AUTH0_AUDIENCE=https://your-domain.auth0.com/api/v2/
AUTH0_DOMAIN=your-domain.auth0.com

# Kafka Configuration
KAFKA_BROKER=kafka:9092
KAFKAJS_NO_PARTITIONER_WARNING=1

# VersaPay Configuration (SENSITIVE - Use your actual values)
VERSAPAY_API_TOKEN=your_versapay_api_token_here
VERSAPAY_BASE_URL=https://api-sandbox.versapay.com
